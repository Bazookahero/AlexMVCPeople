@model MVC_People.Models.ViewModels.CreateLanguageViewModel

<h1>Create new language</h1>

<form asp-action="_NewLanguagePartial" asp-controller="Language" method="post">
    <div asp-validation-summary="ModelOnly"></div>
    <h4>Name of language</h4>
    <input asp-for="LanguageName" type="text" />
    <span asp-validation-for="LanguageName"></span>
    <button type="submit" value="NewLanguage" @*onclick="history.go(-1); return false;"*@>Submit</button>
</form>